{
  "name": "sample_application_cypress",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --env type=actual --headless --browser chrome",
    "cypress:snapshots": "cypress run --env type=base --headless --browser chrome"
  },
  "eslintConfig": {
    "extends": [
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "canvas": "2.9.0",
    "cypress": "12.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "resemblejs": "^4.1.0",
    "sanitize-filename": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cypress-visual-regression-resemble-js": "^1.0.0",
    "prettier": "^2.8.1"
  }
}
